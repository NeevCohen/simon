/dts-v1/;
/plugin/;

/ {
	fragment@0 {
		target-path = "/";
		__overlay__ {
			led {
				compatible = "led";
				label = "led";
				status = "okay";
				led-gpios = <&gpio 17 0>, // blue
				            <&gpio 27 0>, // red
				            <&gpio 22 0>, // green
				            <&gpio 23 0>; // yellow

				btn-gpios = <&gpio 5 1>, // blue-btn
				            <&gpio 6 1>, // red-btn
				            <&gpio 26 1>, // green-btn
				            <&gpio 24 1>; // yellow-btn
			};
		};
	};
};
